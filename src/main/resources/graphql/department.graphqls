type Query {
   departments: [Department]
   department(id: ID!): Department!
}

type Mutation {
   newDepartment(department: DepartmentRequestDto!): Department
}

input DepartmentRequestDto {
   name: String!
   organizationId: Int
}

extend type Mutation {
   newDepartmentFill(department: DepartmentRequestDto!): Department
   generateDepartments(count: Int!): [Department]
}

type Department {
   id: ID!
   name: String!
   organization: Organization
   employees: [Employee]
}